# combat
on('pr1');
hit{
  {:(?(a>b){dec(a.hp,10);})
    {:a;v:d(6);}
    {:b;v:d(4);}
  }
}
shell{
  {:(?(a>b){iam('hit');parent(a,b);do(me.pr1);})
    {:a;v:d(6);}
    {:b;v:d(4);}
  }
}
iam(shell);parent(a,b);do(me.pr1);
